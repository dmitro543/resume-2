<main class="p-5">
    <div class="card">
        <div class="card-body">
            {{#with shopcart}}
               {{#with navigation}}
                      {{#each links}}
                         <p class="btn btn-primary">text: {{text}}</p>
                         <b class="btn btn-danger">href: {{href:}}</b>
                         <br>
                         <h1 class="text-success">text: {{text}}</h1>
                         <h2 class="text-warning">href: {{href}}</h2>
                         <br>
                         <span class="text-info">text: {{text}}</span>
                         <samp class="btn btn-outline-primary">href: {{href}}</samp>
                      {{/each}}
                {{/with}}
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with header}}
                           <h3 class="btn btn-warning">title: {{title}}</h3>
                           {{#each button}}
                              <h4 class="btn btn-outline-success">text: {{text}}</h4>
                              <h5 class="text-success">link: {{link}}</h5>
                           {{/each}}
                        {{/with}}
                    </div>
                </div>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with goodsBlock}}
                           <h6 class="text-warning">image: {{image}}</h6>
                           <article class="text-center">name: {{name}}</article>
                           <var class="text-danger"> description: {{ description}}</var>
                           <pre class="text-end">price: {{price}}</pre>
                           <p class="text-muted">amount: {{amount}}</p>
                           {{#each buttons}}
                              <h5 class="text-uppercase">text: {{text}}</h5>
                              <h6 class="text-lowercase">text: {{text}}</h6>
                           {{/each}}
                        {{/with}}
                    </div>
                </div>
            {{/with}}
        </div>
    </div>
</main>
