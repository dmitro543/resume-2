{{#with mac}}
    {{#with data}}
        <div>
          <p>name: {{name}}</p>
          <h1>description: {{description}}</h1>
          <h2>price:{{price}}</h2>
          <h3>:category {{category}}</h3>
        </div>
    {{/with}} 
      <br>
    {{#each features}}
      {{#with processor}}
        <div>
          <span>brand: {{brand}}</span>
          <samp>model: {{model}}</samp>
          <b>speed:{{speed}}</b>
          <p>cores: {{cores}}</p>
          <article>cache: {{cache}}</article>
        </div>
      {{/with}}
    {{/each}}    
     <br>
        <div>
          {{#with memory}}
            <h4>type: {{type}}</h4>
            <h5>size: {{size}}</h5>
            <h6>:speed {{speed }}</h6>
          {{/with}}
          <br>
          {{#with storage}}
            <var>type: {{type}}</var>
            <abbr>size:{{size}}</abbr>
          {{/with}}
        </div>
{{/with}}
