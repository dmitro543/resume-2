<!DOCTYPE html>
<html>
    <head> 
    <body>
    </body>
    </head>
 </html>
  <div>
    <header>
     <div>
       <h1>{{name.firstname}} {{name.lastname}}</h1>
     </div>
     <div>
       <b>{{position}}</b>
     </div>
     <br>
     <div>
       <em>{{salary}}</em>
     </div>
     <br>
     <hr>
    </header>
  </div>
<div>

  {{#with main}}
    <main>
        <section>
            {{>title data=summary.title big=true}}
            <div>
                <p>
                  {{summary.text}}
                </p>
            </div>
        </section>

     {{#with expirience}}
        <section>
            {{>title data=title}}
           <div>
                <p>
                  {{text}}
                </p>
           </div>
        </section>
      {{/with}}
    </main>
  {{/with}}
  <footer>
    <br>
    <hr>
    {{#with social}}
       {{>link data=email}}
       {{>link data=phone}}
       {{>link data=linkedin}}
    {{/with}}
  </footer>
</div>
  