{{#with car}}
    {{#with data}}
      <div>
        <p>make:{{make}}</p>
        <h1>model:{{model}}</h1>
        <h2>year:{{year}}</h2>
        <h3>color:{{color}}</h3>
      </div>
    {{/with}} 
      <br>

    {{#each features}}
       {{#with interior}}
         {{#with seats}}
              <div>
                <span>material:{{material}}</span>
                <samp>color:{{color}}</samp>
                <b>heated:{{heated}}</b>
                <p>ventilated:{{ventilated}}</p>
              </div>
         {{/with}}
       {{/with}}
    {{/each}}
        <br>
    {{#each deashboard}}
       <div>
          <h4>material:{{material}}</h4>
          <h5>color:{{color}}</h5>
          <h6>type:{{type}}</h6>
          <var>size:{{size}}</var>
          <abbr>resolution:{{resolution}}</abbr>
          <ul>touchscreen:{{touchscreen}}</ul>
       </div>
    {{/each}} 
      <br>
{{/with}}

